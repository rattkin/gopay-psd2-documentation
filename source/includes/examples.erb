
<%
data.languages.each do |l|
    lang = l.syntax
%>

<% example %>

<% if example.has_key?(lang) %>
<%= partial "includes/code",
    :locals => { :language => lang, :example => example[lang] } %>
<% end %>

<% end %>

<% if example.has_key?('json') %>
<%= partial "includes/code",
    :locals => { :language => 'json', :example => example['json'] } %>
<% end %>